<pl:metadata-config xmlns:pl="http://www.exlibrisgroup.com/Plugins/1.0/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" >
  <pl:pluginTypeName>PublisherRegistryPlugin</pl:pluginTypeName>
  <pl:deployName>FS-Publisher</pl:deployName>
  <pl:className>com.exlibris.dps.repository.plugin.registry.FSPublisherPlugin</pl:className>
  <pl:initParameters>
	<fr:x_form xmlns:fr="http://com/exlibris/digitool/common/forms/xmlbeans">
		<form_name>DBPublisherForm</form_name>
		<description>FigShare Publisher Form</description>
		<grid_x>1</grid_x>
		<md_format>1</md_format>
		<x_fields>
			<x_field>
				<field_name>profile.publisherParams(baseUrl)</field_name>
				<label>ui.configuration_profile_publisher.figshre.url</label>
				<ui_tool_tip>ui.configuration_profile_publisher.figshre.url</ui_tool_tip>
				<single>true</single>
				<default_value>https://api.figsh.com/v2/account/articles/</default_value>
				<pos_x>1</pos_x>
				<pos_y>1</pos_y>
				<mandatory>true</mandatory>
				<x_logic_type>String</x_logic_type>
				<x_ui_type>TextField</x_ui_type>
				<x_options/>
			</x_field>
			<x_field>
				<field_name>profile.publisherParams(token)</field_name>
				<label>ui.configuration_profile_publisher.figshre.token</label>
				<ui_tool_tip>ui.configuration_profile_publisher.figshre.token</ui_tool_tip>
				<single>true</single>
				<default_value></default_value>
				<pos_x>1</pos_x>
				<pos_y>4</pos_y>
				<mandatory>true</mandatory>
				<x_logic_type>String</x_logic_type>
				<x_ui_type>TextField</x_ui_type>
				<x_options/>
			</x_field>
			<x_field>
				<field_name>profile.publisherParams(name)</field_name>
				<label>ui.configuration_profile_publisher.figshre.name</label>
				<ui_tool_tip>ui.configuration_profile_publisher.figshre.name</ui_tool_tip>
				<single>true</single>
				<default_value>Rosetta</default_value>
				<pos_x>1</pos_x>
				<pos_y>5</pos_y>
				<mandatory>true</mandatory>
				<x_logic_type>String</x_logic_type>
				<x_ui_type>TextField</x_ui_type>
			</x_field>
		</x_fields>
	</fr:x_form>
  </pl:initParameters>
  <pl:description>FSPublisherPlugin</pl:description>
  <pl:version>1</pl:version>
  <pl:materialType>DIGITAL</pl:materialType>
  <pl:module>Repository</pl:module>
  <pl:generalType>TASK</pl:generalType>
  <pl:publicApi>N</pl:publicApi>
  <pl:implType>java</pl:implType>
  <pl:contactPerson>
    <contact_info xmlns="http://www.exlibrisgroup.com/Plugins/1.0/">
      <contactType>admin</contactType>
      <firstName>Exlibris</firstName>
      <lastName>Ltd</lastName>
      <telephone1>(050)746-1234</telephone1>
      <telephone2></telephone2>
      <email>shaib@exlibris.co.il</email>
      <address1>Agodat Asport2 Building 9</address1>
      <address2></address2>
      <city>Jerusalem</city>
      <zipCode>00000</zipCode>
      <country>Israel</country>
    </contact_info>
  </pl:contactPerson>
</pl:metadata-config>
